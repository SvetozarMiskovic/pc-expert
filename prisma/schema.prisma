generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL_DEV")
}

model korisnici {
  id             Int     @id @unique(map: "id_UNIQUE") @default(autoincrement())
  ime_i_prezime  String? @db.VarChar(45)
  email          String? @unique(map: "email_UNIQUE") @db.VarChar(45)
  broj_telefona  Int?
  postanski_broj Int?
  adresa         String? @db.VarChar(45)
  ulica          String? @db.VarChar(45)
  lozinka        String? @db.VarChar(90)
  grad           String? @db.VarChar(45)
  role           String? @db.VarChar(45)
}

model laptopi {
  id              Int     @id @unique(map: "idlaptopi_UNIQUE")
  proizvodjac     String? @db.VarChar(255)
  model           String? @db.VarChar(255)
  procesor        String? @db.VarChar(255)
  ram             String? @db.VarChar(45)
  garancija       String? @db.VarChar(45)
  hard_disk       String? @db.VarChar(100)
  cijena          String? @db.VarChar(45)
  graficka        String? @db.VarChar(45)
  tip_procesora   String? @db.VarChar(45)
  os              String? @db.VarChar(45)
  boja            String? @db.VarChar(45)
  tip_hard_diska  String? @db.VarChar(45)
  tip_ram         String? @db.VarChar(45)
  velicina_ekrana String? @db.VarChar(45)
  detalji         String? @db.VarChar(255)
}
